  /* text space */
  .text :
  {
    *(.init0)  /* Entry Point, initialize signals handling */
    *(.init1)
    *(.init2)  /* Clear r0 and set up stack pointer r7 */
    *(.init3)
    *(.init4)  /* Initialize data and BSS */
    *(.init5)
    *(.init6)  /* C++ constructors */
    *(.init7)
    *(.init8)
    *(.init9)  /* Call main() */

	*(.text)
    *(.text.*)
