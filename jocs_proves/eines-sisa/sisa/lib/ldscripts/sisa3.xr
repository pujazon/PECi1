    *(.fini7)
    *(.fini6)  /* C++ destructors.  */
    *(.fini5)
    *(.fini4)
    *(.fini3)
    *(.fini2)
    *(.fini1)
    *(.fini0)  /* Halt CPU */
    *(.signals)  /* signals handling  */
  }
  .data	0 :
  {
    *(.data)
    *(.gnu.linkonce.d*)
  }
  .bss  :
  {
    *(.bss)
    *(COMMON)
  }
  /* Global data not cleared after reset */
